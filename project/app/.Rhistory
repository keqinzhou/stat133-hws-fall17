}
to_kelvin <- function(x=1){
(x+459.67)*(5/9)
}
to_reaumur <- function(x=1){
(x-32)*(4/9)
}
to_rankine <- function(x=1){
x+459.67
}
temp_convert <- function(x=1,to='celsius'){
switch(tolower(to),
'celsius'=to_celsius(x),
'kelvin'=to_kelvin(x),
'reaumur'=to_reaumur(x),
'rankine'=to_rankine(x))
}
temp_convert(30,'celsius')
temp_convert(30,'Celsius')
temp_convert(30,'CELSIUS')
